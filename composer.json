    "type": "craft-plugin",
    "version": "1.0.0",
    "keywords": [
        "craft",
        "cms",
        "craftcms",
        "craft-plugin",
        "quotes"
    ],
    "support": {
        "docs": "https://github.com/https://github.com/emukupa/quotes/quotes/blob/master/README.md",
        "issues": "https://github.com/https://github.com/emukupa/quotes/quotes/issues"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Manda",
            "homepage": "https://staplegun.us/"
        }
    ],
    "require": {
        "craftcms/cms": "^3.0.0-RC1"
