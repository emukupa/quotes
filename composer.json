    "autoload": {
        "psr-4": {
          "emukupa\\quotes\\": "src/"
        }
    },
    "extra": {
        "name": "quotes",
        "handle": "quotes",
        "hasCpSettings": false,
        "hasCpSection": false,
        "changelogUrl": "https://raw.githubusercontent.com/https://github.com/emukupa/quotes/quotes/master/CHANGELOG.md",
